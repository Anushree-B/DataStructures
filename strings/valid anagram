word1=input("Enter first word: ")
word2=input("Enter second word: ")

dict1={}
dict2={}

for i in word1:
    count=0
    for j in word1:
        if i==j:
            count=count+1
    dict1[i]=count

for i in word2:
    count=0
    for j in word2:
        if i==j:
            count=count+1
    dict2[i]=count

if dict1==dict2:
    print("It is an anagram.")
else:
    print("It is not an anagram.")
